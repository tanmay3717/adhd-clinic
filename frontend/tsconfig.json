  "compilerOptions": {
    "target": "ES5",
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true
  },
  "include": [
    "src/**/*"
  ]
}
